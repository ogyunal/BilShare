{
  "extends": "vaadin",
  "rules": {
    "linebreak-style": 0,
    "object-curly-spacing": 0

  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "Polymer": false,
    "Vaadin": false,
    "moment": false,
    "DateUtilsMixin": false,
    "ScrollShadowMixin": false
  },
  "parserOptions": {
      "sourceType": "module"
  }
}
